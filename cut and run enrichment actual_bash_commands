time python3 cr_enrichment_actual.py\
 -a CHM13_kmer_counts.jf\                           #Kmer counts from reference genome (.jf input) 
 -G Illumina_Genomic/SRR1997411_kmer_counts.jf\     #Kmer counts for Illumina_Genomic data (.jf input)
